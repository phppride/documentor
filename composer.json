{
    "name": "phppride/html5",
    "description": "An HTML5 parser and serializer conforming to the specification",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Phppride\\Html5\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Phppride\\Html5\\Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Mezinov",
            "email": "ivanmezinov@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "phppride/documentor-contracts": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
